{
  "env": {
    "es6": true,
    "react-native/react-native": true
  },
  "parser": "babel-eslint",
  "extends": [
    "plugin:react-native/recommended",
    "standard"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "react-native"
  ],
  "rules": {}
}
